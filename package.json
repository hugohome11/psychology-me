{
  "name": "psychology-me",
  "private": true,
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
  "db:push": "pnpm --filter @app/web exec prisma db push",
  "db:generate": "pnpm --filter @app/web exec prisma generate",
    "db:check": "pnpm --filter @app/web tsx scripts/db-check.ts",
    "health:local": "curl -fsSL http://localhost:3000/api/health && echo"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "tsx": "^4.19.0"
  }
}
